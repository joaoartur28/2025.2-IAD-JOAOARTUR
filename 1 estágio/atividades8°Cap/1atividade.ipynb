    "def corte(lista):\n",
    "    if len(lista) >= 2:\n",
    "        del lista[0]\n",
    "        del lista[-1]\n",
    "    return None\n",
    "\n",
    "\n",
    "def meio(lista):\n",
    "    return lista[1:-1]\n"
   ]
